<?xml version="1.0" encoding="UTF-8" ?>
<routes xmlns="http://symfony.com/schema/routing"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://symfony.com/schema/routing
        http://symfony.com/schema/routing/routing-1.0.xsd">
    <route id="l91_seo.crawl.overview" path="/">
        <default key="_controller">L91SeoBundle:Crawl:overview</default>
    </route>

    <route id="l91_seo.crawl.detail" path="/{id}">
        <default key="_controller">L91SeoBundle:Crawl:detail</default>
    </route>

    <route id="l91_seo.crawl.url" path="/{id}/urls/{urlId}">
        <default key="_controller">L91SeoBundle:Crawl:url</default>
    </route>
</routes>
